    napi_delete_reference
    napi_fatal_error
    napi_get_and_clear_last_exception
    napi_get_cb_info
    napi_get_last_error_info
    napi_get_reference_value
    napi_is_exception_pending
    napi_module_register
    napi_open_handle_scope
    napi_set_property
    napi_throw
